[alias]
    lg = log -n 10 --color --graph --pretty=format:'%C(cyan bold)%h%Creset - %C(blue)%an%Creset, %ar%C(yellow)%d%Creset%n %C(normal dim italic)%s%Creset'    
    poule = !git pull && echo '🐔'
    staged = diff --name-only --cached
    nope = reset --soft HEAD~
[core]
    editor = /usr/bin/vim
[init]
    defaultBranch = master
[pull]
    rebase = true
